{
  "canisters": {
    "credits": {
      "main": "src/credits/main.mo",
      "type": "motoko"
    },
    "serve": {
      "dependencies": [
        "credits"
      ],
      "frontend": {
        "entrypoint": "src/serve/public/index.js"
      },
      "source": [
        "src/serve/assets",
        "dist/serve/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.6.12",
  "networks": {
    "ic": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
